<Window x:Class="RejectionSampling.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RejectionSampling"
        mc:Ignorable="d"
        Title="MainWindow" Height="460" Width="810">


    <Canvas
        x:Name="canvas">
        <Canvas.RenderTransform>
            <ScaleTransform ScaleX="1.0" ScaleY="-1.0" CenterX="0" CenterY="210"/>
        </Canvas.RenderTransform>
        <Line
                X1="0.0"
                Y1="0.0"
                X2="0"
                Y2="450"
                Stroke="Black"
                StrokeThickness="4"></Line>
        <Line
                X1="0.0"
                Y1="0.0"
                X2="800"
                Y2="0"
                Stroke="Black"
                StrokeThickness="4"></Line>

    </Canvas>




</Window>
